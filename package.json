{
  "name": "viber",
  "version": "0.4.0",
  "type": "module",
  "description": "Agent Framework for Vibe-working",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "build": "pnpm build:lib && pnpm build:docs",
    "build:lib": "tsup && pnpm build:svelte",
    "build:svelte": "svelte-package -i src/svelte -o dist/svelte",
    "build:docs": "cd docs && pnpm build",
    "start": "pnpm start:docs",
    "start:docs": "cd docs && pnpm preview",
    "dev": "pnpm run dev:lib & pnpm run dev:docs",
    "dev:lib": "tsup --watch",
    "dev:docs": "cd docs && pnpm dev",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "multi-agent",
    "ai",
    "collaboration",
    "react",
    "svelte",
    "llm",
    "agent"
  ],
  "author": "Tiwater",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    },
    "./svelte": {
      "types": "./dist/svelte/index.d.ts",
      "svelte": "./dist/svelte/index.js",
      "default": "./dist/svelte/index.js"
    }
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "svelte": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/deepseek": "^0.2.12",
    "@ai-sdk/openai": "^1.3.22",
    "@openrouter/ai-sdk-provider": "^0.7.0",
    "@supabase/supabase-js": "^2.49.1",
    "ai": "^4.3.16",
    "nanoid": "^3.3.8",
    "reflect-metadata": "^0.2.2",
    "yaml": "^2.8.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/node": "^22.10.6",
    "@types/react": "^19.2.6",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.3.5",
    "tsup": "^8.4.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  }
}
