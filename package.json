{
  "name": "@dustland/viber",
  "version": "0.4.3",
  "type": "module",
  "description": "Agent Framework for Vibe-working",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "viber": "./dist/cli/index.js"
  },
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "dev": "concurrently -n hub,web,viber \"tsx watch src/cli/index.ts hub\" \"cd web && pnpm dev\" \"tsx watch src/cli/index.ts start\"",
    "dev:hub": "tsx watch src/cli/index.ts hub",
    "dev:viber": "tsx watch src/cli/index.ts start",
    "dev:web": "cd web && pnpm dev",
    "build": "tsup",
    "build:web": "cd web && pnpm build",
    "start": "node dist/cli/index.js start",
    "viber": "node dist/cli/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "preview:web": "cd web && pnpm preview",
    "verify:skills": "tsx scripts/verify-skills.ts"
  },
  "keywords": [
    "multi-agent",
    "ai",
    "collaboration",
    "llm",
    "agent",
    "daemon"
  ],
  "author": "Dustland",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.0",
    "@ai-sdk/deepseek": "^2.0.8",
    "@ai-sdk/openai": "^3.0.0",
    "@openrouter/ai-sdk-provider": "^2.0.2",
    "@supabase/supabase-js": "^2.49.1",
    "ai": "^6.0.0",
    "commander": "^14.0.2",
    "croner": "^9.1.0",
    "dotenv": "^17.2.3",
    "nanoid": "^3.3.8",
    "reflect-metadata": "^0.2.2",
    "ws": "^8.19.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "@types/ws": "^8.18.1",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.0.0",
    "jsdom": "^26.1.0",
    "tsup": "^8.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  }
}
